{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2022",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@simpel-page/about": ["libs/simpelPage/about-page/src/index.ts"],
      "@simpel-page/blog": ["libs/simpelPage/blog-page/src/index.ts"],
      "@simpel-page/contact": ["libs/simpelPage/contact-page/src/index.ts"],
      "@simpel-page/content": ["libs/simpelPage/content-page/src/index.ts"],
      "@simpel-page/datenschutz": [
        "libs/simpelPage/datenschutz-page/src/index.ts"
      ],
      "@simpel-page/faq": ["libs/simpelPage/faq-page/src/index.ts"],
      "@simpel-page/footer-feature": [
        "libs/simpelPage/footer-feature/src/index.ts"
      ],
      "@simpel-page/impressum": ["libs/simpelPage/impressum-page/src/index.ts"],
      "@simpel-page/landing": ["libs/simpelPage/src/index.ts"],
      "@simpel-page/landing-page": [
        "libs/simpelPage/landing-page/src/index.ts"
      ],
      "@simpel-page/main-content": [
        "libs/simpelPage/main-content-page/src/index.ts"
      ],
      "@simpel-page/nav-feature": ["libs/simpelPage/nav-feature/src/index.ts"],
      "@simpel-page/newsletter": [
        "libs/simpelPage/newsletter-page/src/index.ts"
      ],
      "@simpel-page/offer": ["libs/simpelPage/offer-page/src/index.ts"],
      "@simpel-page/src": ["libs/simpelPage/landing/src/index.ts"],
      "@simpel-page/toggle-menu": [
        "libs/simpelPage/toggle-menu-feature/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
